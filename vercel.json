{
    "version": 2,
    "buildCommand": "next build",
    "outputDirectory": ".next",
    "env": {
        "NODE_OPTIONS": "--max-old-space-size=4096"
    },
    "framework": "nextjs",
    "regions": [
        "all"
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                }
            ]
        }
    ]
}